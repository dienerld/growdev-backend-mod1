{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Diener Dornelas <dienezim@hotmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "swc ./src/ -d dist/",
    "dev": "tsnd --respawn --transpile-only --ignore-watch  node_modules -r tsconfig-paths/register src/infra/http/server.ts",
    "start": "ts-node src/index.ts",
    "test": "jest --watch",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:up": "typeorm-ts-node-commonjs migration:run -d ./src/infra/database/data-source.ts",
    "typeorm:down": "typeorm-ts-node-commonjs  migration:revert -d ./src/infra/database/data-source.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "0.3.10"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11",
    "@swc/jest": "^0.2.23",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.11.10",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.0.1",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.2.2",
    "rimraf": "^3.0.2",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.5.2"
  }
}
